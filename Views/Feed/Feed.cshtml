@model IEnumerable<Article>
@{
    ViewBag.Title = "Посты";
}

<div class="container">
    <a asp-controller="Feed" asp-action="Edit">Создать</a>
</div>

<table>
    @foreach (var article in Model)
    {
        <tr>
            <td>@article.Title</td>
            <td>
                <a asp-controller="Feed" asp-action="Edit" asp-route-id="@article.Id">Изменить</a> |
                <a asp-controller="Feed" asp-action="Delete" asp-route-id="@article.Id">Удалить</a>
            </td>
        </tr>
    }
</table>